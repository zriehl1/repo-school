optimize false;
def octuple_std =
@n
  2*2*2*n;

def addten_std = 
@x
  let a = 2*3 in
  let b = 1*1+1*1+1*2 in
  a+b+x;

def alwaystwo_std = 
@a
  if true then
    2
  else
    a+5;

